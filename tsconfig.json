{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["app/components/*"],
      "@screens/*": ["app/screens/*"],
      "@mock/*": ["app/mock/*"],
      "@dictionaries/*": ["app/dictionaries/*"],
      "@contexts/*": ["app/contexts/*"],
      "@utils/*": ["app/utils/*"],
      "@app/*": ["app/*"]
    }
  }
}